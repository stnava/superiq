{
    "version": 1,
    "aws_profile": "role",
    "job_definition" : "superiq_ppmi",
    "job_queue" : "superiq_ppmi",
    "skip_repeats": true,
    "parameters": {
        "environment" : "prod",
        "process_name": "ppmi",
        "input_bucket" : "ppmi-image-data",
        "input_value" : "tests/ADNI_test/002_S_4473/20140227/T1w/000/ADNI-002_S_4473-20140227-T1w-000.nii.gz",
        "pipeline_bucket": "mjff-ppmi",
        "pipeline_prefix": "superres-pipeline-v01/",
        "template_bucket": "invicro-pipeline-inputs",
        "template_key": "adni_templates/adni_template.nii.gz",
        "template_label_key" : "adni_templates/adni_template_dkt_labels.nii.gz",
        "model_bucket": "invicro-pipeline-inputs", 
        "model_key": "models/SEGSR_32_ANINN222_3.h5",
        "atlas_bucket": "invicro-pipeline-inputs",
        "atlas_image_prefix": "OASIS30/Brains/", 
        "atlas_label_prefix": "OASIS30/Segmentations/", 
        "wlab": [36,55,57],
        "seg_params": { 
            "submask_dilation": 8,
            "reg_iterations": [100,100,20],
            "searcher": 1,
            "radder": 2,
            "syn_sampling": 32,
            "syn_metric": "mattes",
            "max_lab_plus_one": true,
            "verbose": true
        },
        "seg_params_sr": { 
            "submask_dilation": 8,
            "reg_iterations": [100,100,20],
            "searcher": 1,
            "radder": 2,
            "syn_sampling": 32,
            "syn_metric": "mattes",
            "max_lab_plus_one": true,
            "verbose": true
        },
        "sr_params":{
            "upFactor": [2,2,2],
            "dilation_amount": 8,
            "verbose": true
        },
        "output_file_prefix": "ppmi",    
        "output_bucket" : "mjff-ppmi",
        "output_prefix": "superres-pipeline-v01/",
        "output_suffix": "-ppmi-ppmi_srOnNativeSeg"
    }
}
