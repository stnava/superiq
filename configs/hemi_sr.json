{
    "version": 1,
    "aws_profile": "role",
    "job_definition" : "superiq_hemi_sr",
    "job_queue" : "superiq_hemi_sr",
    "skip_repeats": false,
    "parameters": {
        "environment" : "prod",
        "process_name": "hemi_sr",
        "input_bucket" : "superiq-general",
